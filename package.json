    "@eslint/js": "^9.17.0",
    "@types/chokidar": "^1.7.5",
    "@types/diff": "^6.0.0",
    "@types/diff-match-patch": "^1.0.36",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
