{
  "name": "athanor",
  "version": "0.5.1",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "dev": "cross-env NODE_ENV=development concurrently \"webpack serve --config webpack.renderer.config.js\" \"webpack --config webpack.preload.config.js --watch\" \"npm run start\"",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "build:win": "cross-env NODE_ENV=production electron-forge make --platform win32",
    "lint": "eslint ./src --ext .ts,.tsx",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "electron",
    "ai-coding",
    "llm",
    "developer-tool",
    "coding-assistant",
    "prompt-engineering",
    "prompt-builder",
    "code-integration",
    "llm-chat-integration",
    "typescript",
    "react",
    "local-workflow",
    "context-management",
    "desktop-app"
  ],
  "author": "Luigi Acerbi",
  "license": "Apache-2.0",
  "description": "Electron app for developers that orchestrates local code context into effective AI prompts, streamlining interaction with LLM chat interfaces for efficient and controlled code integration.",
